{
  "apps": [{
    "name": "vidcatch-pro-dev",
    "script": "npm",
    "args": "run dev",
    "cwd": "/home/user/webapp",
    "env": {
      "NODE_ENV": "development",
      "PORT": 3000,
      "HOST": "0.0.0.0"
    },
    "watch": false,
    "instances": 1,
    "autorestart": true,
    "max_memory_restart": "1G",
    "error_file": "./logs/err.log",
    "out_file": "./logs/out.log",
    "log_file": "./logs/combined.log",
    "time": true
  }]
}